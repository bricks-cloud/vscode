{
  "name": "d2c-vscode",
  "publisher": "bricks",
  "displayName": "d2c-vscode",
  "description": "Design to code tool",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "views": {
      "explorer": [
        {
          "id": "bricksWorkspace",
          "name": "Bricks Workspace"
        }
      ]
    },
    "commands": [
      {
        "command": "bricksDesignToCode.openFile",
        "title": "Open File"
      },
      {
        "command": "bricksDesignToCode.openWebview",
        "title": "Open Webview",
        "category": "Bricks"
      },
      {
        "command": "bricksDesignToCode.handleUri",
        "title": "Start handling Uris"
      },
      {
        "command": "preview.show",
        "title": "Show preview of currently opened file"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "yarn run compile:view && yarn run compile:extension",
    "compile:extension": "tsc --outDir out",
    "compile:view": "webpack",
    "watch": "yarn run watch:view && yarn run watch:extension",
    "watch:extension": "tsc --watch --outDir out",
    "watch:view": "webpack --watch",
    "watch-tests": "tsc -w --outDir out",
    "package": "webpack --mode production --devtool hidden-source-map",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mkdirp": "^0.5.2",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.19",
    "@types/prettier": "^2.7.2",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/rimraf": "^2.0.2",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.3",
    "webpack-cli": "^5.0.0"
  },
  "dependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "babel-loader": "^9.1.2",
    "css-loader": "^6.7.3",
    "file-loader": "^6.2.0",
    "mkdirp": "^0.5.1",
    "postcss-loader": "^7.0.2",
    "postcss-preset-env": "^8.0.0",
    "prettier": "^2.8.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^2.6.2",
    "socket.io": "^4.5.4",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.2.4",
    "webpack": "^5.75.0",
    "webpack-dev-server": "^4.11.1"
  }
}
